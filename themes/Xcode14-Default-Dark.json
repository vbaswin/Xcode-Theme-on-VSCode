        },
        "typeParameter.declaration": {
            "foreground": "#6BDFFF"
        },
        "typeAlias.declaration": {
            "foreground": "#6BDFFF"
        },
        "namespace.declaration": {
            "foreground": "#6BDFFF"
        },
        "variable.declaration": {
            "foreground": "#4EB0CC"
        },
        "parameter.declaration": {
            "foreground": "#4EB0CC"
        },
        "variable.constant.declaration": {
            "foreground": "#4EB0CC"
        },
        "variable.static.declaration": {
            "foreground": "#4EB0CC"
        },
        "enumMember.declaration": {
            "foreground": "#4EB0CC"
        },
        "lifetime.declaration": {
            "foreground": "#4EB0CC"
        },
        "property.declaration": {
            "foreground": "#4EB0CC"
        },
        "function.declaration": {
            "foreground": "#4EB0CC"
        },
        "method.declaration": {
            "foreground": "#4EB0CC"
        },
        "constParameter.declaration": {
            "foreground": "#4EB0CC"
        },
        "type": {
            "foreground": "#ACF2E4"
        },
        "class": {
            "foreground": "#ACF2E4"
        },
        "struct": {
            "foreground": "#ACF2E4"
        },
        "enum": {
            "foreground": "#ACF2E4"
        },
        "union": {
            "foreground": "#ACF2E4"
        },
        "interface": {
            "foreground": "#ACF2E4"
        },
        "typeParameter": {
            "foreground": "#ACF2E4"
        },
        "typeAlias": {
            "foreground": "#ACF2E4"
        },
        "namespace": {
            "foreground": "#ACF2E4"
        },
        "type.library": {
            "foreground": "#DABAFF"
        },
        "class.library": {
            "foreground": "#DABAFF"
        },
        "struct.library": {
            "foreground": "#DABAFF"
        },
        "enum.library": {
            "foreground": "#DABAFF"
        },
        "union.library": {
            "foreground": "#DABAFF"
        },
        "interface.library": {
            "foreground": "#DABAFF"
        },
        "typeParameter.library": {
            "foreground": "#DABAFF"
        },
        "typeAlias.library": {
            "foreground": "#DABAFF"
        },
        "namespace.library": {
            "foreground": "#DABAFF"
        },
        "builtinType": {
            "foreground": "#DABAFF"
        },
        "function": {
            "foreground": "#78C2B3"
        },
        "method": {
            "foreground": "#78C2B3"
        },
        "function.library": {
            "foreground": "#B281EB"
        },
        "method.library": {
