        },
        "arithmetic": {
            "foreground": "#B281EB"
        },
        "bitwise": {
            "foreground": "#B281EB"
        },
        "logical": {
            "foreground": "#B281EB"
        },
        "comparison": {
            "foreground": "#B281EB"
        },
        "variable.constant": {
            "foreground": "#78C2B3"
        },
        "variable.static": {
            "foreground": "#78C2B3"
        },
        "enumMember": {
            "foreground": "#78C2B3"
        },
        "constParameter": {
            "foreground": "#78C2B3"
        },
        "lifetime": {
            "foreground": "#78C2B3",
            "bold": false,
            "italic": false,
            "underline": false
        },
        "variable.constant.library": {
            "foreground": "#B281EB"
        },
        "variable.static.library": {
            "foreground": "#B281EB"
        },
        "enumMember.library": {
            "foreground": "#B281EB"
        },
        "property": {
            "foreground": "#78C2B3"
        },
        "property.library": {
            "foreground": "#B281EB"
        },
        "macro": {
            "foreground": "#FFA14F"
        },
        "derive": {
            "foreground": "#FFA14F"
        },
        "macro.library": {
            "foreground": "#FFA14F"
        },
        "derive.library": {
            "foreground": "#FFA14F"
        },
        "magit-ref-name": {
            "foreground": "#78C2B3"
        },
        "magit-remote-ref-name": {
            "foreground": "#ACF2E4"
        },
        "unresolvedReference": {
            "foreground": "#F32C2C",
            "bold": false,
            "italic": false,
            "underline": false
        },
        "variable": {
            "foreground": "#FFFFFFD8"
        },
        "parameter": {
            "foreground": "#FFFFFFD8"
        },
        "macroBang": {
            "foreground": "#FFFFFFD8"
        },
        "*.mutable": {
            "underline": true
        }
    }
}